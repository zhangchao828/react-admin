    "father": "4.5.3",
    "prettier": "^2.2.1",
    "tailwindcss": "3.4.17",
    "ts-loader": "^9.5.2"
  },
  "dependencies": {
    "antd": "5.22.5",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
